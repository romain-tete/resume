{
  "routes": [
    {
      "route": "/fr/assets/*"
    },
    {
      "route": "/fr/*",
      "serve": "/fr/index.html"
    },
    {
      "route": "/en/assets/*"
    },
    {
      "route": "/en/*",
      "serve": "/en/index.html"
    },
    {
      "route": "/*",
      "serve": "/en/",
      "statusCode": 301
    }
  ]
}
