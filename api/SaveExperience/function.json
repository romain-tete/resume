{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["put"],
      "route": "experiences/{id}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "CosmosDBConnection",
      "direction": "in",
      "name": "experience",
      "databaseName": "resume",
      "collectionName": "experiences",
      "connectionStringSetting": "CosmosDBConnection",
      "id": "{id}",
      "partitionKey": "test"
    },
    {
      "type": "CosmosDBConnection",
      "direction": "out",
      "name": "saveExperienceBinding",
      "databaseName": "resume",
      "collectionName": "experiences",
      "createIfNotExists": "false",
      "connectionStringSetting": "CosmosDBConnection",
      "partitionKey": "pk"
    }
  ],
  "scriptFile": "../dist/SaveExperience/index.js"
}
