{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "api": {
      "matchCondition": { "route": "/api/{*restOfPath}" },
      "backendUri": "https://api.xcedia.fr/{restOfPath}",
      "requestOverrides": {
        "backend.request.headers.Authorization": "{request.headers.Authorization}"
      }
    }
  }
}
